<h1> Search Results</h1>

<% @teams.each do |team| %>
  <div class="team">
    <h1 class="team-name"><%= link_to team.name, team %></h1>
    <p><%= team.stadium %> </p>
  </div>
<% end %>

<p> No Results? Search by Team name Again</p>
<%= form_tag(teams_path, :method => "get", class: "navbar-form", id: "search-form") do %>
<div class="input-append">
  <%= text_field_tag :search, params[:search], class: "span2", placeholder: "Search For Teams" %>
  <% end %>
